var cm = ",";
var a = "one,two,three,four,five,six,seven";
var b = a.split(",");
var c = b.shift();
var d = b.shift();
var e = b.join("<<>>")+cm+'false'+cm+c+cm+d

// console.log(e);
function parse (v) {
  var x = v.shift();
  var y = v.shift();
  return ret([x,y,v])
}

bind(ret(a))
(x => ret(x.split(",")))
(v => parse(v))
(z => ret(z[0] + ',' + z[1] + ',' + z[2].join("<<>>")),"$mm2")


